                <th class="js-filter-sort" data-filter="isUp" data-dir="ASC"><span>Is Up</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="isResponsive" data-dir="ASC"><span>Is Responsive</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="responseTime" data-dir="ASC"><span>Response Time</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="requestTime" data-dir="ASC"><span>Request Time</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="statusCode" data-dir="ASC"><span>Status Code</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="job" data-dir="ASC"><span>Job</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="tags" data-dir="ASC"><span>Tags</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="error" data-dir="ASC"><span>Error</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="endpoint" data-dir="ASC"><span>Endpoint</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
            </tr>
        </thead>
        <tfoot>
            <tr>
                <th class="js-filter-sort" data-filter="uuid" data-dir="ASC"><span>Uuid</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="id" data-dir="ASC"><span>Id</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="isUp" data-dir="ASC"><span>Is Up</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="isResponsive" data-dir="ASC"><span>Is Responsive</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="responseTime" data-dir="ASC"><span>Response Time</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="requestTime" data-dir="ASC"><span>Request Time</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="statusCode" data-dir="ASC"><span>Status Code</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="job" data-dir="ASC"><span>Job</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="tags" data-dir="ASC"><span>Tags</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="error" data-dir="ASC"><span>Error</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
                <th class="js-filter-sort" data-filter="endpoint" data-dir="ASC"><span>Endpoint</span>
                    <button class="btn btn-icon active"><span class="glyphicon glyphicon-sort-by-attributes"></span></button>
                </th>
            </tr>
        </tfoot>

        <tbody>
        <%
            locals.records && locals.records.forEach((item)=>{ %>
                <tr>
                    <td>
                        <%= item.uuid %>
                    
                    </td>
                    <td>
                        
                        <a href="<%= urlFor('GET', 'check', item.id); %>">
                            <%= item.id %>
                        </a>
                    
                    </td>
                    <td>
                        <%= item.isUp %>
                    
                    </td>
                    <td>
                        <%= item.isResponsive %>
                    
                    </td>
                    <td>
                        <%= item.responseTime %>
                    
                    </td>
                    <td>
                        <%= item.requestTime %>
                    
                    </td>
                    <td>
                        <%= item.statusCode %>
                    
                    </td>
                    <td>
                        <% if(item.job){ %>
                            <a class="job" href="<%= urlFor('GET', 'job', item.job.id); %>">
                                <%= item.job.name || item.job.id %>
                            </a>
                        <% } else { %>
                            ---
                        <%}%>
                    
                    </td>
                    <td>
                        <%= item.tags %>
                    
                    </td>
                    <td>
                        <%= item.error %>
                    
                    </td>
                    <td>
                        <%= item.endpoint %>
                    
                    </td>
                </tr>
        <% }) %>
        </tbody>
    </table>
