<div id="Application-form-content">
    <form id="Application-main-form" method="<%= method %>" action="<%= action %>"  class="form">
            <div class="form-group">
                <label for="uuid">Uuid</label>
                <input type="text" name="uuid"  
                    id="application_uuid"
                    placeholder="Uuid"
                    value="<%= record.uuid ? record.uuid : '' %>"
                    class="form-control"
                    />
            </div>
    
        <div class="form-group">
            <label for="id">Id</label>
            <input type="text" name="id"  
                id="application_id"
                placeholder="Id"
                <% if(crudAction !== 'create'){ %>
                disabled="disabled"
                <% } %>
                value="<%= record.id ? record.id : '' %>"
                class="form-control"
                />
        </div>
    
            <div class="form-group">
                <label for="identifier">Identifier</label>
                <input type="text" name="identifier"  
                    id="application_identifier"
                    placeholder="Identifier"
                    value="<%= record.identifier ? record.identifier : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="appId">App Id</label>
                <input type="text" name="appId"  
                    id="application_appId"
                    placeholder="App Id"
                    value="<%= record.appId ? record.appId : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="hostname">Hostname</label>
                <input type="text" name="hostname"  
                    id="application_hostname"
                    placeholder="Hostname"
                    value="<%= record.hostname ? record.hostname : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="environment">Environment</label>
                <input type="text" name="environment"  
                    id="application_environment"
                    placeholder="Environment"
                    value="<%= record.environment ? record.environment : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="online">Online</label>
                <input type="" name="online"  
                    id="application_online"
                    placeholder="Online"
                    value="<%= record.online ? record.online : 'false' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="jobs">Jobs</label>
                <select id="application_jobs"
                    name="jobs"
                    data-reluri="<%=  %>"
                    data-crudaction="<%= locals.crudAction %>"
                    data-uri="<%= urlFor(locals.crudAction, '/job'); %>"
                    data-rel="many-to-one"
                    value="<%= record.jobs ? record.jobs : '' %>"
                    class="js-select-autopopulate form-control multiselect"
                    multiple>
                
                </select>
            </div>
    
            <div class="form-group">
                <label for="data">Data</label>
                <input type="text" name="data"  
                    id="application_data"
                    placeholder="Data"
                    value="<%= record.data ? record.data : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="healthUrl">Health Url</label>
                <input type="text" name="healthUrl"  
                    id="application_healthUrl"
                    placeholder="Health Url"
                    value="<%= record.healthUrl ? record.healthUrl : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="healthInterval">Health Interval</label>
                <input type="text" name="healthInterval"  
                    id="application_healthInterval"
                    placeholder="Health Interval"
                    value="<%= record.healthInterval ? record.healthInterval : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="label">Label</label>
                <input type="text" name="label"  
                    id="application_label"
                    placeholder="Label"
                    value="<%= record.label ? record.label : '' %>"
                    class="form-control"
                    />
            </div>
    
            <div class="form-group">
                <label for="description">Description</label>
                <input type="text" name="description"  
                    id="application_description"
                    placeholder="Description"
                    value="<%= record.description ? record.description : '' %>"
                    class="form-control"
                    />
            </div>
    

        <% if(record.id){ %>
            <input hidden="true" name="id" value="<%= record.id %>" required/>
        <% } %>

        <div class="pull-right">
        <% if(record.id){ %>
        <button
            type="button"
            value="Delete"
            class="btn btn-danger">
                Delete
        </button>            
        <% } %>
        <button
            type="submit"
            value="Save"
            class="btn btn-primary">
                Save
        </button>
        <!-- this should be a conditional -->
        <button
            type="reset"
            value="Cancel"
            class="btn btn-default">
                Cancel
        </button>
        </div>
    </form>
</div>
